{
  "name": "lwk",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generate:types": "turbo run generate:types && cd packages/types && pnpm sync:types",
    "types:sync": "cd packages/types && pnpm build:types",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:coverage": "turbo run test:coverage"
  },
  "devDependencies": {
    "@supabase/mcp-server-supabase": "^0.4.1",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.14.0",
  "pnpm": {
    "overrides": {
      "@types/react": "^19.1.6",
      "@types/react-dom": "^19.1.2"
    }
  }
}