{
  "name": "@lwk/admin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--inspect\" next dev --port 3001",
    "devturbo": "pnpm dev --turbo",
    "devsafe": "rm -rf .next && pnpm dev",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" next build",
    "start": "cross-env NODE_OPTIONS=\"--inspect\" next start --port 3001",
    "lint": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" next lint",
    "lint:fix": "pnpm lint --fix",
    "clean": "rm -rf .next && rm -rf node_modules",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:importmap": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:importmap",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.40.0",
    "@payloadcms/live-preview-react": "^3.40.0",
    "@payloadcms/next": "^3.40.0",
    "@payloadcms/payload-cloud": "^3.40.0",
    "@payloadcms/plugin-form-builder": "^3.40.0",
    "@payloadcms/plugin-nested-docs": "^3.40.0",
    "@payloadcms/plugin-redirects": "^3.40.0",
    "@payloadcms/plugin-search": "^3.40.0",
    "@payloadcms/plugin-seo": "^3.40.0",
    "@payloadcms/richtext-lexical": "^3.40.0",
    "@payloadcms/ui": "^3.40.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/leaflet": "^1.9.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.476.0",
    "mapbox-gl": "^3.12.0",
    "next": "^15.2.3",
    "next-intl": "^4.1.0",
    "payload": "^3.40.0",
    "prism-react-renderer": "^2.4.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.1",
    "sharp": "0.32.6",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.0.3",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^20.14.9",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
