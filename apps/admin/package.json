{
  "name": "@lwk/admin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--inspect\" next dev --port 3000",
    "devturbo": "pnpm dev --turbo",
    "devsafe": "rm -rf .next && pnpm dev",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" next build",
    "start": "cross-env NODE_OPTIONS=\"--inspect\" next start --port 3000",
    "lint": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" next lint",
    "lint:fix": "pnpm lint --fix",
    "clean": "rm -rf .next && rm -rf node_modules",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:importmap": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:importmap",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.0.0-beta.117",
    "@payloadcms/payload-cloud": "3.0.0-beta.117",
    "@payloadcms/plugin-form-builder": "3.0.0-beta.117",
    "@payloadcms/plugin-nested-docs": "3.0.0-beta.117",
    "@payloadcms/plugin-redirects": "3.0.0-beta.117",
    "@payloadcms/plugin-search": "3.0.0-beta.117",
    "@payloadcms/plugin-seo": "3.0.0-beta.117",
    "@payloadcms/richtext-lexical": "3.0.0-beta.117",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "next": "15.0.3",
    "payload": "3.0.0-beta.117",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.0.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.14.9",
    "@types/react": "npm:types-react@19.0.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
